{
  "name": "Ueact",
  "version": "0.0.1",
  "description": "scaffold for creating engineering vue.js project ",
  "author": "wxyyxc1992",
  "license": "MIT",
  "homepage": "https://github.com/wxyyxc1992/create-webpack-app",
  "repository": {
    "type": "git",
    "url": "https://github.com/wxyyxc1992/create-webpack-app"
  },
  "keywords": [
    "boilerplate",
    "live",
    "hot",
    "reload",
    "vue.js",
    "hmr",
    "edit",
    "webpack",
    "babel",
    "PostCSS(FlexBox Polyfill)"
  ],
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start:dll && better-npm-run start:dev",
    "build": "npm run clean && better-npm-run build",
    "build:umd": "npm run clean && rollup -c rollup.config.js > dist/ueact.umd.min.js",
    "build:ssr": "npm run clean && better-npm-run build:ssr && better-npm-run build:server",
    "clean": "rimraf dist/*",
    "lint": "eslint src",
    "update": "npm-check -u"
  },
  "betterScripts": {
    "start:dll": {
      "command": "./node_modules/.bin/webpack  --config ./dev-config/webpack.dll.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:dev": {
      "command": "webpack-dashboard -- node --harmony ./dev-config/server/devServer.js --host 0.0.0.0",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "./node_modules/.bin/webpack --config ./dev-config/webpack.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:ssr": {
      "command": "./node_modules/.bin/webpack --config ./dev-config/webpack.config.js",
      "env": {
        "NODE_ENV": "ssr"
      }
    },
    "build:server": {
      "command": "./node_modules/.bin/webpack --config ./dev-config/webpack.server.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "./dev-config/tool/.eslintrc.js"
  },
  "babel": {
    "extends": "./dev-config/tool/.babelrc"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "devDependencies": {
    "@types/classnames": "^2.2.0",
    "@types/ramda": "^0.24.4",
    "@types/react": "^15.0.38",
    "autoprefixer": "7.1.1",
    "awesome-typescript-loader": "^3.2.1",
    "babel-core": "6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-plugin-dynamic-import-node": "1.0.2",
    "babel-plugin-import": "1.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "^0.0.15",
    "bundle-loader": "^0.5.5",
    "chai": "4.0.2",
    "chalk": "^2.0.1",
    "commander": "^2.10.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-spawn": "5.1.0",
    "css-loader": "0.28.4",
    "cssnano": "^3.10.0",
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-vue": "^2.1.0",
    "express": "4.15.3",
    "extract-text-webpack-plugin": "3.0.0-beta.3",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "image-webpack-loader": "3.3.1",
    "imagemin": "5.3.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "lint-staged": "^4.0.1",
    "node-sass": "4.5.3",
    "object-assign": "^4.1.1",
    "offline-plugin": "4.8.1",
    "postcss-flexibility": "2.0.0",
    "postcss-loader": "^2.0.6",
    "prepack-webpack-plugin": "^1.1.0",
    "prettier": "^1.5.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "sass-loader": "^6.0.6",
    "scss-loader": "^0.0.1",
    "stack-source-map": "^1.0.6",
    "style-loader": "0.18.2",
    "underscore-template-loader": "^0.7.3",
    "url-loader": "0.5.9",
    "vue-loader": "^12.1.0",
    "vue-template-compiler": "^2.3.3",
    "wasm-loader": "^1.0.0",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "2.8.2",
    "webpack-dashboard": "0.4.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "fluent-fetcher": "^0.2.8",
    "jquery": "^3.2.1",
    "observer-x": "^0.0.7",
    "ramda": "^0.24.1",
    "vue": "^2.4.1"
  }
}
